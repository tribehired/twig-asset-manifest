{
    "name": "tribehired/twig-asset-manifest",
    "type": "library",
    "description": "Json asset manifest mapping.",
    "require": {
        "twig/extensions": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "TribeHired\\TwigAssetManifest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TribeHired\\TwigAssetManifest\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "scripts": {
        "test": "phpunit --configuration ./phpunit.xml ./tests",
        "lint": "phpcs --standard=PSR2 --parallel=3 -p src/",
        "lint-fix": "phpcbf --standard=PSR2 --parallel=3 -p src/"
    }
}